{
  "name": "rangex-backend",
  "version": "0.1.0",
  "description": "RangeX NestJS backend with MySQL, cost-aware lab orchestration, and security-first defaults.",
  "license": "MIT",
  "scripts": {
    "start": "node dist/src/main.js",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "seed": "node -r ts-node/register ./scripts/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "npm run build && npx typeorm-ts-node-commonjs migration:run -d dist/typeorm.config.js",
    "migration:generate": "npx typeorm-ts-node-commonjs migration:generate -d typeorm.config.ts",
    "endpoints:create": "ts-node src/scripts/manage-vpc-endpoints.ts create",
    "endpoints:delete": "ts-node src/scripts/manage-vpc-endpoints.ts delete",
    "endpoints:list": "ts-node src/scripts/manage-vpc-endpoints.ts list",
    "endpoints:tighten": "ts-node src/scripts/manage-vpc-endpoints.ts tighten"
  },
  "dependencies": {
